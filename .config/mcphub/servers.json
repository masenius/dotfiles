{
  "mcpServers": {
    "bin": {
      "autoApprove": [
        "get_topics",
        "get_topic",
        "get_connectors",
        "get_connector"
      ],
      "disabled": true,
      "disabled_tools": [],
      "url": "http://localhost:8000/mcp/"
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "github": {
      "disabled": true,
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "kafka": {
      "args": [
        "mcp-remote",
        "http://localhost:8080/kafka-ui-admin/mcp/sse",
        "--allow-http",
        "--transport sse-only"
      ],
      "command": "npx",
      "disabled": true
    }
  }
}
